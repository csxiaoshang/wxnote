.Title_Text{
  display: flex;
  color: black;
  font-size: 18px;
  width: 80%;
}
.lab{
  width: 24px;
  height: 24px;
}
.AllIn{
  display: flex;
  justify-content: space-between;
  margin-left: 15px;
  margin-right: 15px;
  padding: 10px;
}
.Head{
  width: 300px;
  display: flex;
  justify-content: flex-start
}
.PictureTo
{
  flex: flex;
  justify-content: flex-end
}
.Input{
  margin-left: 50px;
  margin-right: 50px;
}
.ClickableText{
  display: flex;
  justify-content: center;
  color:#05A081;
  font-size: 20px;
}
.userinfo {
position: relative;
width: 750rpx;
height: 240rpx;
color: #fff;
display: flex;
flex-direction: column;
align-items: center;
}
.userinfo-avatar {
overflow:hidden;
display: block;
width: 160rpx;
height: 160rpx;
margin: 20rpx;
margin-top: 50rpx;
border-radius: 50%;
border: 2px solid #fff;
box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}
.name{
  display: flex;
  justify-content: center;
  font-size: 20px;
}